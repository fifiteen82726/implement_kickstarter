<h1>Project#index</h1>


<%= search_form_for @q do |f| %>

 
  <p>Projects name: </p>
  <%= f.search_field :name_cont %>
<%#= f.submit 'name of button here', :class => 'submit_class_name_here' %>
  <%= f.submit 'Search', :class => 'btn btn-success-outline' %>
<% end %>

<% @projects.each do |project| %>

<h3>
	<%= link_to "#{project.name}", category_project_path(project.category_id,project) %>
	<%#= project.name %>
</h3>
	<br>
<% end %>
