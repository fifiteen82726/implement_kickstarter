<h1>Category#show</h1>
<p>Find me in app/views/category/show.html.erb</p>
<div class="digg_pagination">
<%= will_paginate @projects, :container => false %>
</div>

<%= search_form_for @q do |f| %>

 
  <p>Projects name: </p>
  <%= f.search_field :name_cont %>

  <%= f.submit 'Search', :class => 'btn btn-success-outline' %>
<% end %>

<br>
<% @projects.each do |project| %>
<%= link_to "#{project.name}", category_project_path(@category, project) %>
<br>
<% end %>
